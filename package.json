{
  "name": "prt",
  "version": "1.0.0",
  "private": true,
  "description": "Transform PRs into visible artwork, based on your coding style",
  "author": "DaPaleta",
  "license": "ISC",
  "homepage": "https://github.com//",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "lint": "eslint src/ test/ --ext .js,.ts --max-warnings=0",
    "format:check": "prettier --check src/ test/",
    "format:write": "prettier --write src/ test/",
    "test": "jest --config test/config/jest.config.js"
  },
  "dependencies": {
    "probot": "^13.0.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.0.0",
    "nock": "^14.0.0-beta.5",
    "prettier": "^3.2.5",
    "smee-client": "^2.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">= 18"
  }
}
